{
  "writers" : {
    "discard":{
      "writer":"discard"
    },
    "console":{
      "writer":"console"
    },
    "sfiledev":{
      "writer":"sfile",
      "path":"slog/logdev.log",
      "span":"1m"
    },
    "sfilepub":{
      "writer":"sfile",
      "path":"slog/logpub.log",
      "span":"1m"
    }
  },
  "appenders" : {
    "discard" : {
      "appender":"writer",
      "params": {
        "writer":"discard"
      },
      "filters": {
        "level.limit": {"level": "debug"}
      },
      "layout": "[{logger}] [goid:{goid}] {date:2006-01-02 15:04:05} [{level}] {body} {fields} {file::,short}"
    },
    "console" : {
      "appender":"writer",
      "params": {
        "writer":"console"
      },
      "filters": {
        "level.limit": {"level": "debug"}
      },
      "layout": "[{logger}] [goid:{goid}] {date:2006-01-02 15:04:05} [{level}] {body} {fields} {file::,short}"
    },
    "sfiledev" : {
      "appender":"writer",
      "params": {
        "writer":"sfiledev"
      },
      "filters": {
        "level.limit": {"level": "debug"}
      },
      "layout": "[goid:{goid}] {date:2006-01-02 15:04:05} [{level}] {body} {fields} {file::,full}"
    },
    "sfilepub" : {
      "appender":"writer",
      "params": {
        "writer":"sfilepub"
      },
      "filters": {
        "level.limit": {"level": "warn"}
      },
      "layout": "[goid:{goid}] {date:2006-01-02 15:04:05} [{level}] {body} {fields} {file::,full}"
    }
  },
  "loggers" : {
    "loggerdev" : {
      "params": {
      },
      "filters": {
        "level.limit": {"level": "debug"}
      },
      "appenders": ["console", "sfiledev"]
    },
    "loggerpublish" : {
      "params": {
      },
      "filters": {
        "level.limit": {"level": "warn"}
      },
      "appenders": ["sfilepub"]
    }
  }
}